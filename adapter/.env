# Server Configuration
PORT=3000
BODY_LIMIT=1mb
ENABLE_AUTH=true

# WooCommerce Configuration
WOO_BASE_URL=https://your-woocommerce-site.com
WOO_CONSUMER_KEY=your_consumer_key
WOO_CONSUMER_SECRET=your_consumer_secret
WOO_API_VERSION=wc/v3
WOO_API_TIMEOUT=30000

# ONDC Configuration
ONDC_AUTH_TOKEN=your_auth_token
ONDC_SUBSCRIPTION_ID=your_subscription_id
ONDC_PARTICIPANT_ID=your_participant_id
ONDC_BPP_ID=your_bpp_id
ONDC_BPP_URI=https://your-bpp-uri.com
ONDC_CALLBACK_RETRY_COUNT=3
ONDC_CALLBACK_RETRY_DELAY=5000

# ONDC Registry Configuration
ONDC_REGISTRY_URL=https://registry.ondc.org
ONDC_DOMAIN=ONDC:RET10
ONDC_COUNTRY=IND
ONDC_CITY=std:080
ONDC_UK_ID=UKID1

# ONDC Signing Keys (Base64 encoded)
ONDC_SIGNING_PRIVATE_KEY=your_base64_encoded_private_key
ONDC_SIGNING_PUBLIC_KEY=your_base64_encoded_public_key
ONDC_ENCRYPTION_PRIVATE_KEY=your_base64_encoded_encryption_private_key
ONDC_ENCRYPTION_PUBLIC_KEY=your_base64_encoded_encryption_public_key

# Store Configuration
STORE_NAME=WooCommerce Store
STORE_GPS=12.956399,77.636803
STORE_LOCALITY=Main Street
STORE_CITY=Bengaluru
STORE_AREA_CODE=560076
STORE_STATE=Karnataka
STORE_PHONE=9999999999
STORE_EMAIL=store@example.com
STORE_JURISDICTION=Bengaluru
STORE_GST_NUMBER=your_gst_number
STORE_PAN_NUMBER=your_pan_number

# Settlement Configuration
SETTLEMENT_BENEFICIARY_NAME=Your Store Name
SETTLEMENT_UPI_ADDRESS=your-upi@provider
SETTLEMENT_ACCOUNT_NO=your_account_number
SETTLEMENT_IFSC_CODE=your_ifsc_code
